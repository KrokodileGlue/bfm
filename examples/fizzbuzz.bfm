define LIMIT 101;

var f     f = 1;
var i     i = 1;
while f
	var p     p = 1;

	var t
	t = i     t % 3;          not t
	if t      print "Fizz"    p = 0;    end
	
	t = i     t % 5;          not t
	if t      print "Buzz"    p = 0;    end
	
	if p      print i         end
	
	print "\n"

	i + 1;
	f = i     f == LIMIT;     not f
end