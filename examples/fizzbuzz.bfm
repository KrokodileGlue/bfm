var i
var notDone

i = 1
notDone = 1

print 104

print "\"FizzBuzz!\"\nby retrosegfault.tumblr.com\n"

while notDone
	var shouldPrintNum
	shouldPrintNum = 1

	var iModThree
	iModThree = i
	iModThree % 3

	if iModThree
		print "Fizz"
		shouldPrintNum = 0
	endif

	var iModFive
