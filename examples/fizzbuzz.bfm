var i       i = 1
var flag    flag = 1

while flag
	var printNum    printNum = 1
	
	var iModThree   iModThree = i
	iModThree % 3   iModThree == 0

	if iModThree
		print "Fizz"
		printNum = 0
	end

	var iModFive    iModFive = i
	iModFive % 5    iModFive == 0

	if iModFive
		print "Buzz"
		printNum = 0
	end

	if printNum
		print i
	end

	print "\n"

	i + 1

	flag = i        flag == 101
	not flag
end