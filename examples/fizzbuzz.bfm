var i       i = 1
var flag    flag = 1

# LIMIT 101

while flag
	var printNum    printNum = 1
	
	var mod    mod = i
	mod % 3    mod == 0

	if mod
		print "Fizz"
		printNum = 0
	end

	mod = i
	mod % 5    mod == 0

	if mod
		print "Buzz"
		printNum = 0
	end

	if printNum
		print i
	end

	print "\n"

	i + 1

	flag = i        flag == LIMIT
	not flag
end